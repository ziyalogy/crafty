<?xml version="1.0" encoding="utf-8" ?>
<form>
	<fields name="attribs" addfieldpath="/templates/craft/etc/fields">
		<fieldset
      name="extra-fields"
      label="Partner info"
      description=""
      group="extrafields"
    >
			<field
        name="type-article"
        type="hidden"
        default="view-team"
        label="TPL_LISTING_INFO_LABEL"
      />

			<field
        name="spacer"
        type="spacer"
        label="Fill partner information here"
        class="text-label"
      />

	<field
    name="partnerCountry"
    type="list"
    label="TPL_COUNTRY"
    description="FIELDA_DESC"
    >
    <option value="Uganda">Uganda</option>
    <option value="Kenya">Kenya</option>
    <option value="Tanzania">Tanzania</option>
</field>

<field
    name="partnerStatus"
    type="list"
    label="TPL_STATUS"
    description="FIELDA_DESC"
    >
    <option value="active">Active</option>
    <option value="inactive">Inactive</option>
</field>

<field
    name="country_icon"
    type="media"
    label="Country Icon"
    description="FIELDA_DESC"
    >
</field>

<field
    name="crop_icon"
    type="media"
    label="Crop Icon"
    description="FIELDA_DESC"
    >

</field>



	




			

		</fieldset>
	</fields>
</form>
